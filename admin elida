MF 100

comprou  79

dia 26 nov no 59

dia 18 dez 39,50


CF 100

comprou 79

dia 26 nov passou a ser 59

ate hj


select vendas.esn, vendas.os, vendas.nome, vendas.cpf, vendas.status, vendas.valorAparelho as valorVenda, '79,00' as valorCompra, CONCAT(79-vendas.valorAparelho,',00') as diferenca FROM entradas
INNER JOIN itensEntrada itens ON ( itens.id_entrada = entradas.id_entrada)
INNER JOIN ESNsEntradas esn ON ( itens.id_itensEntrada = esn.id_entrada)
INNER JOIN vendas_clarotv vendas ON  ( (vendas.status='FINALIZADA' OR vendas.status='BOLETO GERADO') && vendas.esn = esn.esn
&& vendas.valorAparelho!='' && vendas.valorAparelho<'79,00')
where entradas.data BETWEEN '2013-10-01 00:00:00' AND '2013-11-25 23:59:59'


select vendas.esn, vendas.os, vendas.nome, vendas.cpf, vendas.status, vendas.valorAparelho, '59,00' as valorCompra, CONCAT(79-vendas.valorAparelho,',00') as diferenca  FROM entradas
INNER JOIN itensEntrada itens ON ( itens.id_entrada = entradas.id_entrada)
INNER JOIN ESNsEntradas esn ON ( itens.id_itensEntrada = esn.id_entrada)
INNER JOIN vendas_clarotv vendas ON  ( (vendas.status='FINALIZADA' OR vendas.status='BOLETO GERADO') && vendas.esn = esn.esn
&& vendas.valorAparelho!='' && vendas.valorAparelho<'59,00')
where entradas.data BETWEEN '2013-11-26 00:00:00' AND '2013-12-18 23:59:59'
